<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="FilletImageView">
        <attr name="topLeftCorner" format="boolean" />
        <attr name="bottomLeftCorner" format="boolean" />
        <attr name="topRightCorner" format="boolean" />
        <attr name="bottomRightCorner" format="boolean" />
        <attr name="filletImageRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="StickyNavigationLayout">
        <attr name="fitStatusBar" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SortTypeView">
        <attr name="sortViewBackground" format="reference|color"/>
        <attr name="showPreferential" format="boolean"/>
        <attr name="showScreen" format="boolean"/>
        <attr name="flowTagBackground" format="reference|color"/>
        <attr name="sortTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="OrderProcessView">
        <attr name="process" format="integer"/> <!-- 1-3 -->
    </declare-styleable>

    <declare-styleable name="TimeLineView">
        <attr name="timelineRadiusDistance" format="dimension"/>
        <attr name="timelineRadius" format="dimension"/>
        <attr name="timelineOtherColor" format="color"/>
        <attr name="timelineCount" format="integer"/>
        <attr name="timelineWidth" format="dimension"/>
        <attr name="timelineMarginTop" format="dimension"/>
    </declare-styleable>
    
    <!-- 城市选择器自定义列表分割线颜色 -->
    <attr format="reference" name="mCpDividerBackground"/>

    <attr name="xui_bottom_sheet_grid_item_icon_marginTop">100dp</attr>

</resources>
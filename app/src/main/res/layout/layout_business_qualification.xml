<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.life.base.views.UiAdapterLinearLayout
            android:id="@+id/my_ll_content_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

            <include
                android:id="@+id/layout_title"
                layout="@layout/title_menu_share" />

            <TextView
                style="@style/text_my_simple_style"
                android:layout_marginStart="@dimen/interval_size_xs"
                android:layout_marginTop="@dimen/interval_size_xs"
                android:text="@string/business_license"
                android:textColor="@color/text_normal"
                android:textSize="30px" />

            <ImageView
                android:id="@+id/img_business_license"
                android:layout_width="match_parent"
                android:layout_height="279px"
                android:layout_marginStart="@dimen/interval_size_xs"
                android:layout_marginTop="40px"
                android:scaleType="fitXY"
                android:layout_marginEnd="@dimen/interval_size_xs" />

            <TextView
                style="@style/text_my_simple_style"
                android:layout_marginStart="@dimen/interval_size_xs"
                android:layout_marginTop="40px"
                android:text="@string/food_business_license"
                android:textColor="@color/text_normal"
                android:textSize="30px" />

            <ImageView
                android:id="@+id/img_food_business_license"
                android:layout_width="match_parent"
                android:layout_height="279px"
                android:layout_marginStart="@dimen/interval_size_xs"
                android:layout_marginTop="40px"
                android:scaleType="fitXY"
                android:layout_marginEnd="@dimen/interval_size_xs" />

        </com.life.base.views.UiAdapterLinearLayout>


    </LinearLayout>
</layout>
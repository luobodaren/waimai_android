<?xml version="1.0" encoding="utf-8"?>
<com.life.base.views.UiAdapterLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/interval_size_xs"
    android:paddingStart="@dimen/interval_size_xs"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/cp_list_item_name"
        android:layout_width="match_parent"
        android:layout_height="?attr/cpListItemHeight"
        android:background="?attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center_vertical"
        android:textColor="?attr/cpListItemTextColor"
        android:textSize="?attr/cpListItemTextSize"
        tools:ignore="RtlSymmetry" />


</com.life.base.views.UiAdapterLinearLayout>
